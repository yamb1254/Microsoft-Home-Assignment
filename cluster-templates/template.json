{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "managedClusters_aks_assignment_name": {
            "defaultValue": "aks-assignment",
            "type": "String"
        },
        "workspaces_DefaultWorkspace_c6bd574e_a182_45f0_a90a_f832f0d62b86_EUS_externalid": {
            "defaultValue": "/subscriptions/c6bd574e-a182-45f0-a90a-f832f0d62b86/resourceGroups/DefaultResourceGroup-EUS/providers/Microsoft.OperationalInsights/workspaces/DefaultWorkspace-c6bd574e-a182-45f0-a90a-f832f0d62b86-EUS",
            "type": "String"
        },
        "publicIPAddresses_1009088c_8626_493a_8835_5b7902e77719_externalid": {
            "defaultValue": "/subscriptions/c6bd574e-a182-45f0-a90a-f832f0d62b86/resourceGroups/MC_Microsoft_Home_Assignment_aks-assignment_israelcentral/providers/Microsoft.Network/publicIPAddresses/1009088c-8626-493a-8835-5b7902e77719",
            "type": "String"
        },
        "userAssignedIdentities_aks_assignment_agentpool_externalid": {
            "defaultValue": "/subscriptions/c6bd574e-a182-45f0-a90a-f832f0d62b86/resourceGroups/MC_Microsoft_Home_Assignment_aks-assignment_israelcentral/providers/Microsoft.ManagedIdentity/userAssignedIdentities/aks-assignment-agentpool",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.ContainerService/managedClusters",
            "apiVersion": "2024-06-02-preview",
            "name": "[parameters('managedClusters_aks_assignment_name')]",
            "location": "israelcentral",
            "sku": {
                "name": "Base",
                "tier": "Free"
            },
            "kind": "Base",
            "identity": {
                "type": "SystemAssigned"
            },
            "properties": {
                "kubernetesVersion": "1.29.8",
                "dnsPrefix": "[concat(parameters('managedClusters_aks_assignment_name'), '-dns')]",
                "agentPoolProfiles": [
                    {
                        "name": "agentpool",
                        "count": 1,
                        "vmSize": "Standard_D2s_v3",
                        "osDiskSizeGB": 128,
                        "osDiskType": "Managed",
                        "kubeletDiskType": "OS",
                        "maxPods": 30,
                        "type": "VirtualMachineScaleSets",
                        "maxCount": 5,
                        "minCount": 1,
                        "enableAutoScaling": true,
                        "powerState": {
                            "code": "Running"
                        },
                        "orchestratorVersion": "1.29.8",
                        "enableNodePublicIP": false,
                        "nodeTaints": [
                            "CriticalAddonsOnly=true:NoSchedule"
                        ],
                        "mode": "System",
                        "osType": "Linux",
                        "osSKU": "Ubuntu",
                        "upgradeSettings": {
                            "maxSurge": "10%"
                        },
                        "enableFIPS": false,
                        "securityProfile": {
                            "sshAccess": "LocalUser"
                        }
                    },
                    {
                        "name": "userpool",
                        "count": 5,
                        "vmSize": "Standard_B2s",
                        "osDiskSizeGB": 128,
                        "osDiskType": "Managed",
                        "kubeletDiskType": "OS",
                        "maxPods": 10,
                        "type": "VirtualMachineScaleSets",
                        "maxCount": 5,
                        "minCount": 1,
                        "enableAutoScaling": true,
                        "powerState": {
                            "code": "Running"
                        },
                        "orchestratorVersion": "1.29.8",
                        "enableNodePublicIP": false,
                        "mode": "User",
                        "osType": "Linux",
                        "osSKU": "Ubuntu",
                        "upgradeSettings": {
                            "maxSurge": "10%"
                        },
                        "enableFIPS": false,
                        "securityProfile": {
                            "sshAccess": "LocalUser"
                        }
                    }
                ],
                "windowsProfile": {
                    "adminUsername": "azureuser",
                    "enableCSIProxy": true
                },
                "servicePrincipalProfile": {
                    "clientId": "msi"
                },
                "addonProfiles": {
                    "azureKeyvaultSecretsProvider": {
                        "enabled": false
                    },
                    "azurepolicy": {
                        "enabled": true
                    },
                    "omsAgent": {
                        "enabled": true,
                        "config": {
                            "logAnalyticsWorkspaceResourceID": "[parameters('workspaces_DefaultWorkspace_c6bd574e_a182_45f0_a90a_f832f0d62b86_EUS_externalid')]",
                            "useAADAuth": "true"
                        }
                    }
                },
                "nodeResourceGroup": "[concat('MC_Microsoft_Home_Assignment_', parameters('managedClusters_aks_assignment_name'), '_israelcentral')]",
                "enableRBAC": true,
                "supportPlan": "KubernetesOfficial",
                "networkProfile": {
                    "networkPlugin": "azure",
                    "networkPluginMode": "overlay",
                    "networkPolicy": "none",
                    "networkDataplane": "azure",
                    "loadBalancerSku": "Standard",
                    "loadBalancerProfile": {
                        "managedOutboundIPs": {
                            "count": 1
                        },
                        "effectiveOutboundIPs": [
                            {
                                "id": "[parameters('publicIPAddresses_1009088c_8626_493a_8835_5b7902e77719_externalid')]"
                            }
                        ],
                        "backendPoolType": "nodeIPConfiguration"
                    },
                    "podCidr": "10.244.0.0/16",
                    "serviceCidr": "10.0.0.0/16",
                    "dnsServiceIP": "10.0.0.10",
                    "outboundType": "loadBalancer",
                    "podCidrs": [
                        "10.244.0.0/16"
                    ],
                    "serviceCidrs": [
                        "10.0.0.0/16"
                    ],
                    "ipFamilies": [
                        "IPv4"
                    ],
                    "podLinkLocalAccess": "IMDS"
                },
                "identityProfile": {
                    "kubeletidentity": {
                        "resourceId": "[parameters('userAssignedIdentities_aks_assignment_agentpool_externalid')]",
                        "clientId": "460d7f89-44e8-4d00-b903-c448a2c92670",
                        "objectId": "24f2b0f1-49cb-4248-a279-691955f5e655"
                    }
                },
                "autoScalerProfile": {
                    "balance-similar-node-groups": "false",
                    "daemonset-eviction-for-empty-nodes": false,
                    "daemonset-eviction-for-occupied-nodes": true,
                    "expander": "random",
                    "ignore-daemonsets-utilization": false,
                    "max-empty-bulk-delete": "10",
                    "max-graceful-termination-sec": "600",
                    "max-node-provision-time": "15m",
                    "max-total-unready-percentage": "45",
                    "new-pod-scale-up-delay": "0s",
                    "ok-total-unready-count": "3",
                    "scale-down-delay-after-add": "10m",
                    "scale-down-delay-after-delete": "10s",
                    "scale-down-delay-after-failure": "3m",
                    "scale-down-unneeded-time": "10m",
                    "scale-down-unready-time": "20m",
                    "scale-down-utilization-threshold": "0.5",
                    "scan-interval": "10s",
                    "skip-nodes-with-local-storage": "false",
                    "skip-nodes-with-system-pods": "true"
                },
                "autoUpgradeProfile": {
                    "upgradeChannel": "patch",
                    "nodeOSUpgradeChannel": "NodeImage"
                },
                "disableLocalAccounts": false,
                "securityProfile": {},
                "storageProfile": {
                    "diskCSIDriver": {
                        "enabled": true,
                        "version": "v1"
                    },
                    "fileCSIDriver": {
                        "enabled": true
                    },
                    "snapshotController": {
                        "enabled": true
                    }
                },
                "oidcIssuerProfile": {
                    "enabled": false
                },
                "workloadAutoScalerProfile": {},
                "azureMonitorProfile": {
                    "metrics": {
                        "enabled": true,
                        "kubeStateMetrics": {}
                    },
                    "containerInsights": {
                        "enabled": true,
                        "logAnalyticsWorkspaceResourceId": "[parameters('workspaces_DefaultWorkspace_c6bd574e_a182_45f0_a90a_f832f0d62b86_EUS_externalid')]"
                    }
                },
                "metricsProfile": {
                    "costAnalysis": {
                        "enabled": false
                    }
                },
                "nodeProvisioningProfile": {
                    "mode": "Manual"
                },
                "bootstrapProfile": {
                    "artifactSource": "Direct"
                }
            }
        },
        {
            "type": "Microsoft.ContainerService/managedClusters/agentPools",
            "apiVersion": "2024-06-02-preview",
            "name": "[concat(parameters('managedClusters_aks_assignment_name'), '/agentpool')]",
            "dependsOn": [
                "[resourceId('Microsoft.ContainerService/managedClusters', parameters('managedClusters_aks_assignment_name'))]"
            ],
            "properties": {
                "count": 1,
                "vmSize": "Standard_D2s_v3",
                "osDiskSizeGB": 128,
                "osDiskType": "Managed",
                "kubeletDiskType": "OS",
                "maxPods": 30,
                "type": "VirtualMachineScaleSets",
                "maxCount": 5,
                "minCount": 1,
                "enableAutoScaling": true,
                "powerState": {
                    "code": "Running"
                },
                "orchestratorVersion": "1.29.8",
                "enableNodePublicIP": false,
                "nodeTaints": [
                    "CriticalAddonsOnly=true:NoSchedule"
                ],
                "mode": "System",
                "osType": "Linux",
                "osSKU": "Ubuntu",
                "upgradeSettings": {
                    "maxSurge": "10%"
                },
                "enableFIPS": false,
                "securityProfile": {
                    "sshAccess": "LocalUser"
                }
            }
        },
        {
            "type": "Microsoft.ContainerService/managedClusters/agentPools",
            "apiVersion": "2024-06-02-preview",
            "name": "[concat(parameters('managedClusters_aks_assignment_name'), '/userpool')]",
            "dependsOn": [
                "[resourceId('Microsoft.ContainerService/managedClusters', parameters('managedClusters_aks_assignment_name'))]"
            ],
            "properties": {
                "count": 5,
                "vmSize": "Standard_B2s",
                "osDiskSizeGB": 128,
                "osDiskType": "Managed",
                "kubeletDiskType": "OS",
                "maxPods": 10,
                "type": "VirtualMachineScaleSets",
                "maxCount": 5,
                "minCount": 1,
                "enableAutoScaling": true,
                "powerState": {
                    "code": "Running"
                },
                "orchestratorVersion": "1.29.8",
                "enableNodePublicIP": false,
                "mode": "User",
                "osType": "Linux",
                "osSKU": "Ubuntu",
                "upgradeSettings": {
                    "maxSurge": "10%"
                },
                "enableFIPS": false,
                "securityProfile": {
                    "sshAccess": "LocalUser"
                }
            }
        },
        {
            "type": "Microsoft.ContainerService/managedClusters/maintenanceConfigurations",
            "apiVersion": "2024-06-02-preview",
            "name": "[concat(parameters('managedClusters_aks_assignment_name'), '/aksManagedAutoUpgradeSchedule')]",
            "dependsOn": [
                "[resourceId('Microsoft.ContainerService/managedClusters', parameters('managedClusters_aks_assignment_name'))]"
            ],
            "properties": {
                "maintenanceWindow": {
                    "schedule": {
                        "weekly": {
                            "intervalWeeks": 1,
                            "dayOfWeek": "Sunday"
                        }
                    },
                    "durationHours": 4,
                    "utcOffset": "+00:00",
                    "startDate": "2024-10-11",
                    "startTime": "00:00"
                }
            }
        },
        {
            "type": "Microsoft.ContainerService/managedClusters/maintenanceConfigurations",
            "apiVersion": "2024-06-02-preview",
            "name": "[concat(parameters('managedClusters_aks_assignment_name'), '/aksManagedNodeOSUpgradeSchedule')]",
            "dependsOn": [
                "[resourceId('Microsoft.ContainerService/managedClusters', parameters('managedClusters_aks_assignment_name'))]"
            ],
            "properties": {
                "maintenanceWindow": {
                    "schedule": {
                        "weekly": {
                            "intervalWeeks": 1,
                            "dayOfWeek": "Sunday"
                        }
                    },
                    "durationHours": 4,
                    "utcOffset": "+00:00",
                    "startDate": "2024-10-11",
                    "startTime": "00:00"
                }
            }
        }
    ]
}